{
  "app": {
    "checkForUpdates": true,
    "shutdown": {
      "stopGracePeriod": "5m",
      "log": {
        "enabled": true,
        "filePath": "shutdown.log"
      }
    }
  },
  "logger": {
    "level": "info",
    "disableCaller": true,
    "disableStacktrace": false,
    "stacktraceLevel": "panic",
    "encoding": "console",
    "encodingConfig": {
      "timeEncoder": "rfc3339"
    },
    "outputPaths": [
      "stdout"
    ],
    "disableEvents": true
  },
  "inx": {
    "address": "localhost:9029",
    "maxConnectionAttempts": 30,
    "targetNetworkName": ""
  },
  "faucet": {
    "amount": 1000000000,
    "smallAmount": 100000000,
    "maxAddressBalance": 5000000000,
    "tagMessage": "FAUCET",
    "batchTimeout": "2s",
    "bindAddress": "localhost:8091",
    "rateLimit": {
      "enabled": true,
      "period": "5m",
      "maxRequests": 10,
      "maxBurst": 20
    },
    "pow": {
      "workerCount": 4
    },
    "debugRequestLoggerEnabled": false
  },
  "profiling": {
    "enabled": false,
    "bindAddress": "localhost:6060"
  }
}
